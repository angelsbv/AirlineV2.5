@model IEnumerable<AirlineV2._5.Models.ClnRelFlg>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.client)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.flight)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.client.ClnEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.flight.FlgCategory)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ClnFlgID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ClnFlgID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ClnFlgID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
